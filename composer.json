{
    "name": "kris/laravel-form-builder",
    "description": "Laravel form builder - symfony like",
    "keywords": ["laravel", "form", "builder","symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kristijan Husak",
            "email": "husakkristijan@gmail.com"
        }
    ],
    "repositories" : [
        {
            "type": "vcs",
            "url": "https://github.com/christian-classics-ethereal-library/framework.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/christian-classics-ethereal-library/html.git"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "dev-ccel-laravel5-php8-compat",
        "laravelcollective/html": "dev-ccel-laravel5-php8-compat"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kris\\LaravelFormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "FormBuilder": "Kris\\LaravelFormBuilder\\Facades\\FormBuilder"
            }
        }
    },
    "autoload": {
        "psr-0": {
            "Kris\\LaravelFormBuilder": "src/"
        },
        "classmap": [
            "tests/FormBuilderTestCase.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
